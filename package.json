{
  "name": "guile-vscode",
  "displayName": "Guile Language",
  "description": "Guile syntax highlighting and editing",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tohoyn/guile-vscode.git"    
},
"engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "guile",
      "aliases": ["Guile", "guile"],
      "extensions": [".scm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "guile",
      "scopeName": "source.guile",
      "path": "./syntaxes/guile.tmLanguage.json"
    }]
  }
}
